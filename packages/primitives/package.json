{
  "name": "@native-ui-org/primitives",
  "version": "0.5.0",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "react-native": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-native": ">=0.74"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "clsx": "^2.1.1",
    "jest-environment-jsdom": "^30.1.2",
    "tailwind-merge": "^2.5.2",
    "tsup": "^8.3.0",
    "typescript": "^5.5.4"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "echo \"(add eslint)\"",
    "test:web": "jest -c ./jest.config.web.cjs",
    "test:native": "jest -c ./jest.config.native.cjs",
    "test": "jest -c ./jest.config.web.cjs && jest -c ./jest.config.native.cjs"
  },
  "dependencies": {
    "@react-native/babel-preset": "^0.81.1",
    "@testing-library/react-native": "^13.3.3",
    "serializer": "link:react-native-web/jest/serializer"
  }
}
